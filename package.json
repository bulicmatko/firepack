{
  "name": "firepack",
  "version": "0.0.0-semantically-released",
  "description": "Auth wrapper for Firebase applications built with React and Redux",
  "main": "./index.js",
  "files": [
    "./index.js",
    "./LICENSE",
    "./README.md"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "NODE_ENV=development webpack --progress --colors",
    "build:prod": "NODE_ENV=production webpack --progress --colors",
    "check": "npm run check:eslint && npm run check:flow && npm run check:coverage",
    "check:coverage": "nyc --reporter=html --check-coverage --statements=100 --branches=75 --functions=100 --lines=100 npm run test",
    "check:flow": "flow",
    "check:eslint": "eslint ./src ./test",
    "commit": "git-cz",
    "tdd": "npm run test -- --watch",
    "test": "mocha --recursive --compilers=js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bulicmatko/firepack.git"
  },
  "keywords": [
    "firepack",
    "auth",
    "wrapper",
    "firebase",
    "application",
    "react",
    "redux"
  ],
  "author": "Matko Bulic <bulicmatko@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bulicmatko/firepack",
  "bugs": {
    "url": "https://github.com/bulicmatko/firepack/issues"
  },
  "dependencies": {
    "firebase": "^3.1.0",
    "immutable": "^3.8.1",
    "lodash": "^4.13.1",
    "react": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.3"
  },
  "peerDependencies": {
    "firebase": "^3.1.0",
    "immutable": "^3.8.1",
    "lodash": "^4.13.1",
    "react": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "commitizen": "^2.8.2",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "flow-bin": "^0.28.0",
    "ghooks": "^1.3.2",
    "mocha": "^2.5.3",
    "nyc": "^7.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run check"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=4.2.1 <6",
    "npm": ">=3.10.3 <4"
  }
}
